<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Matteo Flora</title>
 <link href="http://mgpf.it/atom.xml" rel="self"/>
 <link href="http://mgpf.it/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://mgpf.it/</id>
 <author>
   <name>Matteo Flora</name>
   <email>io@mgpf.it</email>
 </author>
 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://mgpf.it{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://mgpf.it{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
   <author>
     <name>Matteo Flora</name>
     <uri>http://mgpf.it/chisono.html</uri>
   </author>
 </entry>
 {% endfor %}
</feed>